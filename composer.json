{
    "name": "marshmallow/nova-fontawesome",
    "description": "A Font Awesome 6 Field for Laravel Nova 4",
    "keywords": [
        "laravel",
        "nova",
        "nova 4",
        "fontawesome 6",
        "fontawesome"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0|^8.1",
        "laravel/nova": "^4.0|^5.0",
        "outl1ne/nova-translations-loader": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Marshmallow\\NovaFontAwesome\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Marshmallow\\NovaFontAwesome\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
